package _20181030;

public class KeyboardInputEx {
    public static void main(String[] args) {
        int c;
        InputStreamReader rd = new InputStreamReader(System.in);

        try {
            while ((c=rd.read()) != -1) {
                System.out.print((char) c);
            }
        } catch (IOExpection e) {
            e.printStackTrace();
        }
    }
}
