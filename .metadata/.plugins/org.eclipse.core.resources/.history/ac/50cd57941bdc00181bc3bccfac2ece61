package _20181030;

import java.io.FileInputStream;

public class FileInputStreamEx {
	public static void main(String[] args) {
		byte b[] = new byte[6];
		try {
			FileInputStream fin = new FileInputStream("D:/CodingData/2018.java/JavaStudy/src/_20181030/fileoutput1.txt");
			int n = 0, c;
			while ((c = fin.read()) != -1) {
				b[n] = (byte) c;
				n++;
			}
		}
	}
}
